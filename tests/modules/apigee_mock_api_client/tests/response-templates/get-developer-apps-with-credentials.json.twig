{#
/**
 * @file
 *   GET /v1/organizations/{org_name}/developers/{developer_email_or_id}/apps?expand=true&includeCred=true
 *
 * Response Code: 200
 *
 * Variables:
 * - org_name:              The name of the org.
 * - developer_email_or_id: The developer email or id.
 */
#}
{
  "apps" : [
    {% for app in apps %}
    {% include 'developer-app.json.twig' with {'app': app, 'credentials': credentials[app.appId] } %}{{ loop.last ? '' : ',' }}
    {% endfor %}
  ]
}
