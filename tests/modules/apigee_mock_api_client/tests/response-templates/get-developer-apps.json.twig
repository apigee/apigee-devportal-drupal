{#
/**
 * @file
 *   GET /v1/organizations/{org_name}/developers/{developer_email_or_id}/apps
 *
 * Response Code: 200
 *
 * Variables:
 * - org_name:              The name of the org.
 * - developer_email_or_id: The developer email or id.
 */
#}
{% if (expand) %}
{
  "apps" : [
    {% for app in apps %}
    {% include 'developer-app.json.twig' with {'app': app} %}{{ loop.last ? '' : ',' }}
    {% endfor %}
  ]
}
{% else %}
[
  {% for app in apps %}
  "{{ app.name }}"{{ loop.last ? '' : ',' }}
  {% endfor %}
]
{% endif %}
