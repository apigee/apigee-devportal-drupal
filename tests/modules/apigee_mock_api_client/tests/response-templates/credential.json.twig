{#
/**
 * @file
 *   Credential
 *
 * Usage:
 *   @code {% include 'credential.json.twig' %} @endcode
 *
 * Variables:
 * - credential: The app credential.
 */
#}
{
  "apiProducts": [
    {% for product in products %}
      {
        "apiproduct": "{{ product.name|default('api_product') }}",
        "status": "{{ product.status|default('approved') }}"
      }{{ loop.last ? '' : ',' }}
    {% endfor %}
  ],
  "attributes": [],
  "consumerKey": "{{ credential.consumerKey|default('dMHy6YlXjGerx7uyZocwP0LOEQgcUSEp') }}",
  "consumerSecret": "{{ credential.consumerSecret|default('dJ8omGPp3CejtANd') }}",
  "expiresAt": 1595289603149,
  "issuedAt": 1594973277149,
  "scopes": [],
  "status": "{{ credential.status|default('approved') }}"
}
